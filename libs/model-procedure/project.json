{
  "name": "model-procedure",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/model-procedure/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/model-procedure",
        "main": "libs/model-procedure/src/index.ts",
        "tsConfig": "libs/model-procedure/tsconfig.lib.json",
        "assets": ["libs/model-procedure/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/model-procedure/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/libs/model-procedure"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/libs/model-procedure"
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional"
      }
    }
  },
  "tags": []
}
